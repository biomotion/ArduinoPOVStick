<Window x:Class="POVStick.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:POVStick"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="372">
    <Grid>
        <Label x:Name="label" Content="請輸入文字" HorizontalAlignment="Left" Margin="10,13,0,0" VerticalAlignment="Top"/>
        <Button x:Name="button" Content="Generate" HorizontalAlignment="Left" Margin="267,15,0,0" VerticalAlignment="Top" Width="75" Height="21"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" Margin="85,14,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="177" TextChanged="textBox_TextChanged"/>
        <TextBox x:Name="textBox1" HorizontalAlignment="Left" Height="331" Margin="10,68,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="332" AcceptsReturn="True"/>
        <Label x:Name="label1" Content="Result" HorizontalAlignment="Left" Margin="10,43,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
